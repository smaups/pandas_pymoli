    "# used this link to see how to use dictionary with group by\n",
    "# https://github.com/rdk5/HeroesOfPymoli/blob/master/Heroes%20of%20Pymoli.ipynb\n",
    "\n",
    "groupgender_df = purchase_data.groupby(['Gender'])\n",
    "####AVERAGE PURCHASE PRICE#####\n",
    "groupgender_price = groupgender_df[\"Price\"].mean().rename(\"Average Purchase Price\")\n",
    "groupgender_price\n",
    "\n",
    "####PURCHASE COUNT#####\n",
    "groupgender_purchase = groupgender_df[\"Purchase ID\"].count().rename(\"Purchase Count\")\n",
    "groupgender_purchase\n",
    "\n",
    "#####TOTAL PURCHASE VALUE#####\n",
    "groupgender_total = groupgender_df[\"Price\"].sum().rename(\"Total Purchase Value\")\n",
    "groupgender_total\n",
    "\n",
    "####AVG TOTAL PURCHASE PER PERSON#####\n",
    "groupgender_average = groupgender_total/gender_df\n",
    "groupgender_average\n",
    "\n",
    "gender_purchasing_analysis = pd.DataFrame({\"Average Price\": groupgender_price,\n",
    "                               \"Purchase Count\": groupgender_purchase,\n",
    "                               \"Total Purchase Value\": groupgender_total,\n",
    "                               \"Average Purchase per Person\": groupgender_average\n",
    "    \n",
    "})\n",
    "gender_purchasing_analysis\n",
    "\n",
    "gender_purchasing_analysis[\"Average Price\"] = gender_purchasing_analysis[\"Average Price\"].map(\"${:.2f}\".format)\n",
    "gender_purchasing_analysis[\"Total Purchase Value\"] = gender_purchasing_analysis[\"Total Purchase Value\"].map(\"${:.2f}\".format)\n",
    "gender_purchasing_analysis[\"Average Purchase per Person\"] = gender_purchasing_analysis[\"Average Purchase per Person\"].map(\"${:.2f}\".format)\n",
    "\n",
    "gender_purchasing_analysis"
   ]
